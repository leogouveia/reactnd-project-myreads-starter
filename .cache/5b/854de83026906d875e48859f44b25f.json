({ books }));\n    });\n  }\n\n  onChangeShelf(book) {\n    let books = this.state.books.filter(b => b.id !== book.id);\n    books.push(book);\n    this.setState(() => ({ books }));\n\n    BooksService.update(book, book.shelf)\n      .then(() => {\n        this.notify('success', 'The book was changed.');\n        return BooksService.getAll();\n      })\n      .catch(() => (book.shelf = book.oldShelf))\n      .then(books => this.setState(() => ({ books })));\n  }\n\n  render() {\n    const { books, shelves } = this.state;\n    return (\n      <Router>\n        <div className=\"app\">\n          <Switch>\n            <Route\n              path=\"/search\"\n              render={() => (\n                <SearchBooks\n                  books={books}\n                  shelves={shelves}\n                  handleChangeShelf={this.onChangeShelf}\n                />\n              )}\n            />\n            <Route\n              path=\"/\"\n              render={() => (\n                <Bookcase\n                  books={books}\n                  shelves={shelves}\n                  handleChangeShelf={this.onChangeShelf}\n                />\n              )}\n            />\n          </Switch>\n        </div>\n      </Router>\n    );\n  }\n}\n\nexport default BooksApp;\n"},"lineCount":null}},"hash":"3b121aff66f2a12d0824cc7ef958796e","cacheData":{"env":{}}}